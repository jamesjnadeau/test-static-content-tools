  "name": "hello-sqlite",
  "version": "0.0.2",
  "description": "A simple Node app with SQLite as a database management system, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:webpack",
    "dev": "npm run dev:webpack",
    "build:functions": "netlify-lambda build functions -c netlify-webpack.config.js",
    "webpack": "webpack --watch --debug --output-pathinfo",
    "build:webpack": "webpack --colors -p --debug --output-pathinfo",
    "netlify-lambda:dev": "netlify-lambda serve functions -c netlify-webpack.config.js",
    "dev:webpack": "webpack-dev-server  --debug --output-pathinfo --host 0.0.0.0 --public $PROJECT_DOMAIN.glitch.me",
    "lint": "eslint ."
  },
