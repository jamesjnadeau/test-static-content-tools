{
  "env": {
    "node": true,
  },
  "extends": "airbnb-base",
  "rules": {
    "strict": [2],
    "no-var": [ 0 ],
    "vars-on-top": [ 0 ],
    "func-names": [ 0 ],
    "no-console": [ 0 ],
    "prefer-arrow-callback": [0],
    "no-param-reassign": [0],
    "comma-dangle": [0],
    "max-len": [0],
    "padded-blocks": [0],
    "prefer-template": [0],
    "dot-notation": [0, { "allowKeywords": false }],
    "object-shorthand": [0],
    "consistent-return": [0],
    "no-unused-vars": [1],
    "no-trailing-spaces": [0],
    "space-before-function-paren": [1, "never"],
    "quotes": [0],
    "camelcase": [0],
    "no-unneeded-ternary": [0],
    "no-alert": [0],
    "no-use-before-define": [0],
    "prefer-rest-params": [0],
    "no-multi-assign": [0],
    "no-underscore-dangle": [0],
    "no-inner-declarations": [0],
    "import/no-extraneous-dependencies": [0],
    "no-plusplus": [0],
    "prefer-destructuring": [0],
    "global-require": [0],
    "no-new": [0],
    "function-paren-newline": [0],
    "no-path-concat": [0],
    "import/no-dynamic-require": [0]
  },
  "globals": {
    "window",
    "$": "jquery",
    "jQuery": "jquery",
    "window.jQuery": "jquery",
    "env",
    "browser",
    "globals",
    "Vue",
    "document",
    "test",
    "fixture"
  },
}
